{
    "name": "vendor/cms-core",
    "description": "Headless CMS core package with Filament admin panel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-model-states": "^2.0",
        "awcodes/filament-tiptap-editor": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\CmsCore\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\CmsCore\\CmsCoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
